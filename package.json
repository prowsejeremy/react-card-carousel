{
  "name": "@jpd.nz/react-card-carousel",
  "version": "0.0.32",
  "description": "A basic card carousel for react, usable with items of either fixed or variable widths.",
  "author": "Jeremy Prowse <jpd.nz> (https://jpd.nz)",
  "license": "MIT",
  "scripts": {
    "build": "npx rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prowsejeremy/react-card-carousel.git"
  },
  "keywords": [
    "react",
    "carousel",
    "slider",
    "typescript",
    "ts"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "bugs": {
    "url": "https://github.com/prowsejeremy/react-card-carousel/issues"
  },
  "homepage": "https://github.com/prowsejeremy/react-card-carousel#readme",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "postcss": "^8.4.39",
    "rollup": "^4.18.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.7",
    "tslib": "^2.6.3",
    "typescript": "^5.3.3"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.es.js",
  "umd": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/types.d.ts",
        "default": "./dist/esm/index.es.js"
      },
      "require": {
        "types": "./dist/cjs/types.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  }
}
